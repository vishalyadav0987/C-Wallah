#include<stdio.h>
int fun();
int main()
{
    int result;
    result = fun();
    printf("%d",result);
    return 0;
}
int fun()
{
    int a,b;
    printf("Enter your 1st value : ");
    scanf("%d",&a);
    printf("Enter your 2nd value : ");
    scanf("%d",&b);  // not running
    return a*b ;
}